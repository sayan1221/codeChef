    "def check(a,l):\n",
    "    c=0\n",
    "    for i in range(0,len(l)):\n",
    "        if(a<l[i]):\n",
    "            c+=1\n",
    "    return c\n",
    "\n",
    "for _ in range(int(input())):\n",
    "    x=list(map(int,input().split()))\n",
    "    l=list(map(int,input().split()))\n",
    "    z=check(x[1],l)\n",
    "    print(z)"
