    "for _ in range(int(input())):\n",
    "    x=int(input())\n",
    "    c=x\n",
    "    if(x==2):\n",
    "        print(1)\n",
    "    else:\n",
    "        for i in range(2,x):\n",
    "            if(x%i==0):\n",
    "                c-=1\n",
    "            elif(x%i!=0):\n",
    "                c+=1\n",
    "        print(c)"
