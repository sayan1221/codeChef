   "source": [
    "n=int(input(\"Enter size : \"))\n",
    "x=list(map(int,input(\"Enter : \").split()))\n",
    "c1=c2=0\n",
    "for i in x:\n",
    "    if(i%2==0):\n",
    "        c1+=1\n",
    "    else:\n",
    "        c2+=1\n",
    "if c1>c2:\n",
    "    print(\"READY FOR BATTLE\")\n",
    "else:\n",
    "    print(\"NOT READY\")"
   ]
